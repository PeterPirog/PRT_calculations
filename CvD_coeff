import numpy as np


def CVD_R(R0,A,B,C,t):
    if t<0:
        R=R0*(1+(A*t)+(B*t**2)+C*(t-100)*t**3)
        return R
    else:
        R=R0*(1+A*t+B*t**2)
    return R

R0=100.139867
A=3.984201E-3
B=-5.86858E-7
C=-6.24817E-12


Tw=np.array([-80.0078,-38.8397,0,231.9287,419.5246,660.323])
R=np.array([67.7848,84.5387,100.1399,189.513,257.2021,337.9695])






